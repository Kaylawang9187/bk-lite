[[inputs.prometheus]]
    urls = ["{{ url }}"]
    interval = "{{ interval | default(10, true) }}s"
    timeout = "{{ timeout | default(5, true) }}s"
    response_timeout = "{{ response_timeout | default(5, true) }}s"
    http_headers = {{ custom_headers | default({}) | to_toml }}
    [inputs.prometheus.tags]
        instance_id = "{{ instance_id }}"
        instance_type = "{{ instance_type }}"
        collect_type = "http"
        config_type = "prometheus"
